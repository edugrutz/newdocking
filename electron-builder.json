{
    "appId": "com.edugrutz.docking",
    "files": [
      "dist-electron",
      "dist-react",
      "resources"
    ],
    "extraResources": [
      {
        "from": "temp",
        "to": "temp",
        "filter": ["**/*"]
      },
      {
        "from": "resources",
        "to": "resources"
      },
      "dist-electron/preload.cjs"
    ],
    "icon": "./icon.png",
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    },
    "win": {
      "target": ["portable", "msi"]
    }
  }